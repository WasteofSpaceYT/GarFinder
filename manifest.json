{
    "name": "Garfield Checker",
    "description": "Checks for Garfield in the page",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "imgs/garfield16.png",
            "24": "imgs/garfield24.png",
            "32": "imgs/garfield32.png"
        }
    },
    "permissions": [
        "tabs",
        "activeTab",
        "scripting"
    ]
}